<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:innerRadius="20dp"
            android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/event_start_blue" />
            <size
                android:width="36dp"
                android:height="36dp" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>

    <item
        android:width="24dp"
        android:height="24dp"
        android:drawable="@drawable/ic_calendar_month"
        android:gravity="center" />
</layer-list>